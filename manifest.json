{
  "manifest_version": 3,
  "name": "Group Tabs by Domain",
  "version": "0.1.0",
  "description": "Group browser tabs by registered domain.",
  "permissions": ["tabs", "storage", "alarms", "tabGroups"],
  "background": { "service_worker": "src/sw.js" },
  "action": { "default_popup": "src/popup.html", "default_icon": "icons/icon.svg" },
  "icons": { "16": "icons/icon.svg", "48": "icons/icon.svg", "128": "icons/icon.svg" }
}
